{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "testTimeout": 60000,
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!uuid)"
  ],
  "globalSetup": "<rootDir>/jest-global-setup.ts",
  "setupFiles": ["<rootDir>/jest-setup.ts"]
}
